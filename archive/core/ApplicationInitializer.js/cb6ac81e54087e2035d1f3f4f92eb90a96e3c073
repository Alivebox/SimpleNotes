/*cb6ac81e54087e2035d1f3f4f92eb90a96e3c073*/Ext.define("ApplicationInitializer",{extend:"Object",init:function(){this.loadAppConfig()},loadAppConfig:function(){var tmpConfigRequest=new XMLHttpRequest();tmpConfigRequest.open("GET","application/data/appconfig.json?",false);tmpConfigRequest.send(null);try{ModelLocator.config=eval("("+tmpConfigRequest.responseText+")")}catch(e){ErrorsManager.handleFatalError(ErrorCodes.APP_LOADING_CONFIG_FILE)}EventBus.fireEvent(FrameworkEvents.APP_INIT_COMPLETE)}});